  <a  name="login"></a>
    <div class="content-section-a">
        <div class="container" id="login">
            <div class="row">
                <div class="col-lg-5 col-sm-6">
                    <hr class="section-heading-spacer">
                    <div class="clearfix"></div>
                    <h2 class="section-heading">Login</h2>
                    <p class="lead">Enter your credentials below to login to use the Job Planner.</p>
                  
                    <form action="/login" role="form" method="post">
                        <div class="form-group">
                            <label>Username : </label>
                            <input id="login_name" type="text" name="username" class="form-control" placeholder="Enter your username" required>
                        </div>
                        <div class="form-group">
                            <label>Password : </label>
                            <input id="login_pass" type="password" name="pass" class="form-control" placeholder="Enter your password" required>
                        </div>
                          <button type="submit" id="login_button" class="btn btn-primary">Submit</button>
                    </form>
            
                    </div>
                <div class="col-lg-5 col-lg-offset-2 col-sm-6">
                    <img class="img-responsive" src="/assets/img/Writer.jpg" alt="">
                </div>
            </div>
        </div>
        <!-- /.container -->

    </div>
    <!-- /.content-section-a -->
    
<a  name="signup"></a>
    <div class="content-section-b">

        <div class="container" id="sign_up">

            <div class="row">
                <div class="col-lg-5 col-lg-offset-1 col-sm-push-6  col-sm-6">
                    <hr class="section-heading-spacer">
                    <div class="clearfix"></div>
                    <h2 class="section-heading">Sign Up</h2>
                    <p class="lead">Enter your credentials below to sign up and use the Job Planner.</p>
                
                    <form action="/signup" role="form" method="post">
                        <div class="form-group">
                            <label>Username: </label>
                                <input id="u_name" type="text" name="username" class="form-control" placeholder="Enter your username" required>
                        </div>
                        <div class="form-group">
                            <label>Password : </label>
                            <input id="e_pass" type="password" name="pass" class="form-control" placeholder="Enter your password" required>
                        </div>
                        <div class="form-group">
                            <label>Confirm Password : </label>
                            <input id="c_pass" type="password" name="confirm_pass" class="form-control" placeholder="Re-enter Password" required>
                        </div>
                        <div class="alert alert-danger calculator-alerts hidden" id="form1-error-message">
                        </div>
                            <button type="submit" id="sign_up_button" class="btn btn-primary">Submit</button>
                    </form>
                </div>
                    <div class="col-lg-5 col-sm-pull-6  col-sm-6">
                        <img class="img-responsive" src="/assets/img/Diary.jpg" alt="">
                    </div>
            </div>
        </div>
        <!-- /.container -->

    </div>
    <!-- /.content-section-b -->

