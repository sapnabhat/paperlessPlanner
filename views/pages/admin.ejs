<div id="page-wrapper">

    <div class="container-fluid">

        <!-- Page Heading -->

        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                    Admin <small>Statistics Overview</small>
                </h1>
                <ol class="breadcrumb">
                    <li class="active">
                        <i class="fa fa-dashboard"></i> Dashboard
                    </li>
                </ol>
            </div>
        </div>
        <!-- /.row --> 

        <div class="row">
        <div class="col-lg-12"> 
        <table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Email </th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <%
       for (var i = 0; i < userList.length; i++) { 
       if (userList[i].username !== username){
        %>
        <tr>
        <td> <%=i+1 %> </td> 
        <td><%= userList[i].username %></td>
        <td><%= userList[i].last_visited %></td>
        <td>
        <% if (userList[i].userStatus == '1') { %>
                    <button class="btn btn-danger user-block" data-id="<%= userList[i]._id %>">Blocked</button>
                    <% } else {%>
                    <button class="btn btn-success user-activate" data-id="<%= userList[i]._id %>">Active</button>
                    <% }%>
        </td>
        </tr>    
    <% } 
    } %>
  </tbody>
</table> 
 </div>
</div> 

    </div>
    <!-- /.container-fluid -->

</div>
<!-- /#page-wrapper -->

